#include <stdio.h>
#include <math.h>

// 3차원 점을 나타내는 구조체 선언
typedef struct {
	double x, y, z;
} Point;

// 두 점 사이의 유클리드 거리 계산 함수
double calculateDistance(Point *p1, Point *p2) {
	return sqrt(pow(p2->x - p1->x, 2) + pow(p2->y - p1->y, 2) + pow(p2->z - p1->z, 2));
}

int main() {
	Point points[2]; // 두 점을 저장할 배열

	// 두 점의 좌표를 한번에 입력받기
	printf("Enter the coordinates of two points (x, y, z):\n");
	for (int i = 0; i < 2; i++) {
		scanf_s("%lf %lf %lf", &points[i].x, &points[i].y, &points[i].z);
	}

	// 두 점 사이의 유클리드 거리 계산
	double distance = calculateDistance(&points[0], &points[1]);

	// 결과 출력
	printf("The Euclidean distance between the two points is: %.2lf\n", distance);

	return 0;
}
